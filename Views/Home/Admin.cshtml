@{
}


<div>
    <h1 style="text-align: center;">Users</h1>
    <br />
</div>

<h2 style="text-align: center;">Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>Username</th>
            <th>Id</th>
            <th>FirstName</th>
            <th>LastName</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var IdentityUser in Model)
        {
            <tr>
                <td>@IdentityUser.Id</td>
                <td>@IdentityUser.Email</td>
                <td>@IdentityUser.UserName</td>
            </tr>
        }
        @foreach (var AboutMe in Model)
        {
            <tr>
                <td>@AboutMe.Id</td>
                <td>@AboutMe.FirstName</td>
                <td>@AboutMe.LastName</td>
            </tr>
        }
    </tbody>
</table>